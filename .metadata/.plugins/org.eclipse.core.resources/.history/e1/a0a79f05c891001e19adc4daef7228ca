

public class Binary {
	public int binarySerch(int arr[], int l, int r, int m) {
		if (r >= l) {
			int mid = l + (r - 1) / 2;s

			if (arr[mid] == m)

				return mid;
			if (arr[mid] > m)

				return binarySerch(arr, l, mid - 1, m);
			return binarySerch(arr, l, mid + 1, m);

		}

		return -1;

	}

	public static void main(String[] args) {
		Binary ob = new BinarySerch();
		int arr[] = { 2, 5, 8, 12, 16, 23, 38, 56, 72, 91 };
		int n = arr.length;
		int m = 23;
		int result;
		if (result == -1) {
			System.out.println("no present");
		} else {
			System.out.println("Prsent");
		}

	}
}
